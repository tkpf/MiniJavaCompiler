class TestClass {
    public void doNothingRecursion(int repetitions)
    {
        if (repetitions == 0)
        {
            return;
        }
        else
        {
            TestClass t = new TestClass();
            t.doNothingRecursion(repetitions - 1);
        }
    }

    public int sumUpRecursive(int num)
        {
            if (num == 0) {
                return 0;
            }
            return sumUpRecursive(num - 1) + num;
        }

}